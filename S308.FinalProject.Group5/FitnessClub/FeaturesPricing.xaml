<Window x:Name="winFeaturesPricing" x:Class="FitnessClub.FeaturesPricing"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FitnessClub"
        mc:Ignorable="d"
        Title="FeaturesPricing" Height="600" Width="600" Background="#FFBF3417">
    <Grid>
        <Image x:Name="image" HorizontalAlignment="Left" Height="100" Margin="23,24,0,0" VerticalAlignment="Top" Width="100" Source="Images/MSNLogo.png"/>
        <Label x:Name="lblSelectFeature" Content="Select Additional Feature:" HorizontalAlignment="Left" Margin="40,155,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="cmbSelectFeatures" HorizontalAlignment="Left" Margin="204,159,0,0" VerticalAlignment="Top" Width="120">
            <ComboBoxItem/>
            <ComboBoxItem Content="Personal Training Plan"/>
            <ComboBoxItem Content="Locker Rental"/>
        </ComboBox>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="249,195,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="button1" Content="Button" HorizontalAlignment="Left" Margin="484,71,0,0" VerticalAlignment="Top" Width="75"/>
        <Label x:Name="label" Content="Label" HorizontalAlignment="Left" Margin="71,269,0,0" VerticalAlignment="Top"/>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="172,279,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
        <Label x:Name="label1" Content="Label" HorizontalAlignment="Left" Margin="88,318,0,0" VerticalAlignment="Top"/>
        <TextBlock x:Name="textBlock1" HorizontalAlignment="Left" Margin="185,327,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
        <Label x:Name="label2" Content="Label" HorizontalAlignment="Left" Margin="301,333,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="385,344,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
